{
  "name": "peynman/larapress-ecommerce",
  "description": "This is the description of the package.",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Peyman Abdi",
      "email": "peyman.abdi.work@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Larapress\\ECommerce\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Larapress\\ECommerce\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Larapress\\ECommerce\\Providers\\PackageServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^7.3.0",
    "laravel/framework": "^8.0",
    "peynman/larapress-profiles": "master",
    "peynman/larapress-reports": "master",
    "jildertmiedema/laravel-plupload": "^0.7.1",
    "brunogasparetto/adobe-connect-client": "master"
  },
  "require-dev": {
    "facade/ignition": "^2.3.6",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^5.0",
    "orchestra/testbench": "^6.2.0",
    "phpunit/phpunit": "^9.0",
    "phpmd/phpmd" : "@stable",
    "squizlabs/php_codesniffer": "3.*",
    "matthewbdaly/artisan-standalone": "dev-master"
  },
  "repositories": {
    "brunogasparetto/adobe-connect-client":{
        "type": "vcs",
        "url": "https://github.com/peynman/AdobeConnectClient.git"
    }
  }
}
