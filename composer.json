{
  "name": "peynman/larapress-ecommerce",
  "description": "Ecommerce for Larapress CRUD.",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Peyman Abdi",
      "email": "larapress@peynman.ir"
    }
  ],
  "autoload": {
    "psr-4": {
      "Larapress\\ECommerce\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Larapress\\ECommerce\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Larapress\\ECommerce\\Providers\\PackageServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^7.0|^8.0",
    "laravel/framework": "^8.0",
    "peynman/larapress-profiles": "master",
    "jildertmiedema/laravel-plupload": "^0.7.1"
  },
  "require-dev": {
    "facade/ignition": "^2.3.6",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^5.0",
    "orchestra/testbench": "^6.2.0",
    "phpunit/phpunit": "^9.0",
    "phpmd/phpmd" : "@stable",
    "squizlabs/php_codesniffer": "3.*"
  },
  "repositories": {
    "peynman/larapress-profiles": {
        "type": "path",
        "url": "../press-profiles",
        "options": {
            "symlink": true
        }
    },
    "peynman/larapress-reports": {
        "type": "path",
        "url": "../press-reports",
        "options": {
            "symlink": true
        }
    }
  }
}
